{
  "type": "system/SIMOS/Application",
  "name": "DMTApplicationSettings",
  "description": "",
  "entities": ["CarPackage", "TodoDemo", "MatrixDemo"],
  "packages": ["CarPackage", "Uncontained", "ComplexArrays", "TodoDemo", "marmo", "mooringSRS"],
  "models": ["system/DMT/Entity"],
  "actions": [
    {
      "name": "Todo Runnable",
      "description": "Get and store all todos",
      "input": "SSR-DataSource/TodoDemo/TodoList",
      "output": "SSR-DataSource/TodoDemo/TodoList",
      "method": "getTodoList",
      "actionType": "resultInEntity"
    },
    {
      "name": "Calculate",
      "description": "Simulate 80kmh concrete crash. Fiat panda 2011.",
      "input": "SSR-DataSource/CarPackage/Car",
      "output": "SSR-DataSource/CarPackage/Car",
      "method": "run",
      "actionType": "resultInEntity"
    },
    {
      "name": "Cancel",
      "description": "Cancel a car calculation",
      "input": "SSR-DataSource/CarPackage/Car",
      "output": "SSR-DataSource/CarPackage/Car",
      "method": "cancel",
      "actionType": "resultInEntity"
    },
    {
      "name": "SRS_Run",
      "type": "system/SIMOS/Action",
      "description": "",
      "input": "SSR-DataSource/mooringSRS/Scenario",
      "output": "SSR-DataSource/mooringSRS/run/RunOutput",
      "method": "srs_run",
      "actionType": "separateResultFile"
    },
    {
        "name": "SRS_Cancel",
        "type": "system/SIMOS/Action",
        "description": "",
        "input": "SSR-DataSource/mooringSRS/run/RunOutput",
        "output": "SSR-DataSource/mooringSRS/run/RunOutput",
        "method": "srs_cancel",
        "actionType": "resultInEntity"
    },
    {
      "name": "Single_Run",
      "type": "system/SIMOS/Action",
      "description": "",
      "input": "SSR-DataSource/mooringSRS/Scenario",
      "output": "SSR-DataSource/mooringSRS/run/RunOutput",
      "method": "single_run",
      "actionType": "separateResultFile"
    },    
    {
      "name": "test_Single_Run",
      "type": "system/SIMOS/Action",
      "description": "",
      "input": "SSR-DataSource/mooringSRS/Scenario",
      "output": "SSR-DataSource/mooringSRS/run/RunOutput",
      "method": "test_single_run",
      "actionType": "separateResultFile"
    },
    {
      "name": "ResultFile",
      "type": "system/SIMOS/Action",
      "description": "Save result in resultFile",
      "input": "SSR-DataSource/ActionsTest/WheelWithStatus",
      "output": "SSR-DataSource/ActionsTest/WheelWithStatus",
      "method": "runResultFile",
      "actionType": "separateResultFile"
    },
    {
      "name": "NoResult",
      "type": "system/SIMOS/Action",
      "description": "No Result Action",
      "input": "SSR-DataSource/ActionsTest/WheelWithStatus",
      "output": "SSR-DataSource/ActionsTest/WheelWithStatus",
      "method": "runNoResult",
      "actionType": "noResult"
    },
    {
      "name": "resultInEntity",
      "type": "system/SIMOS/Action",
      "description": "Save result in entity",
      "actionType": "resultInEntity",
      "input": "SSR-DataSource/ActionsTest/WheelWithStatus",
      "output": "SSR-DataSource/ActionsTest/WheelWithStatus",
      "method": "runResultInEntity"
    }
  ]
}
