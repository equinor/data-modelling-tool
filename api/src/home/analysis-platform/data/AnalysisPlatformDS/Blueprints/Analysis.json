{
  "name": "Analysis",
  "type": "system/SIMOS/Blueprint",
  "extends": [
    "system/SIMOS/DefaultUiRecipes",
    "system/SIMOS/NamedEntity"
  ],
  "description": "",
  "attributes": [
    {
      "name": "label",
      "type": "system/SIMOS/BlueprintAttribute",
      "attributeType": "string",
      "optional": true
    },
    {
      "name": "task",
      "type": "system/SIMOS/BlueprintAttribute",
      "attributeType": "WorkflowDS/Blueprints/Task",
      "contained": true
    },
    {
      "name": "schedule",
      "type": "system/SIMOS/BlueprintAttribute",
      "attributeType": "string",
      "optional": true
    },
    {
      "name": "jobs",
      "type": "system/SIMOS/BlueprintAttribute",
      "attributeType": "WorkflowDS/Blueprints/Job",
      "dimensions": "*"
    },
    {
      "name": "creator",
      "type": "system/SIMOS/BlueprintAttribute",
      "attributeType": "string"
    },
    {
      "name": "description",
      "type": "system/SIMOS/BlueprintAttribute",
      "attributeType": "string",
      "optional": true
    },
    {
      "name": "created",
      "type": "system/SIMOS/BlueprintAttribute",
      "attributeType": "string",
      "optional": true
    },
    {
      "name": "updated",
      "type": "system/SIMOS/BlueprintAttribute",
      "attributeType": "string",
      "optional": true
    }
  ],
  "uiRecipes": [
    {
      "name": "navigator",
      "type": "system/SIMOS/UiRecipe",
      "plugin": "navigator",
      "category": "container"
    },
    {
      "name": "Form",
      "type": "system/SIMOS/UiRecipe",
      "plugin": "form",
      "category": "edit",
      "config": {
        "type": "DMT-Internal/DMT/ui-plugins/form/FormInput",
        "attributes": [
          {
            "type": "DMT-Internal/DMT/ui-plugins/form/fields/StringField",
            "name": "description",
            "widget": "TextareaWidget"
          },
          {
            "type": "DMT-Internal/DMT/ui-plugins/form/fields/StringField",
            "name": "created",
            "widget": "TextWidget",
            "format": "date-time"
          },
          {
            "type": "DMT-Internal/DMT/ui-plugins/form/fields/StringField",
            "name": "updated",
            "widget": "TextWidget",
            "format": "date-time"
          },
          {
            "type": "DMT-Internal/DMT/ui-plugins/form/fields/ArrayField",
            "name": "jobs",
            "columns": ["started", "ended", "triggeredBy"]
          }
        ],
        "order": [
          "name",
          "type",
          "description",
          "created",
          "updated",
          "creator",
          "schedule",
          "label",
          "task",
          "jobs",
          "*"
        ]
      }
    },
    {
      "name": "Tabs",
      "label": "Tabs",
      "type": "system/SIMOS/UiRecipe",
      "plugin": "tabs",
      "category": "container",
      "config": {
        "type": "system/SIMOS/UiPluginSelectorProps"
      }
    }
  ]
}