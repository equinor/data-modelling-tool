{
  "name": "SimaAppService",
  "description": "Configuration for sima application service.",
  "type": "system/SIMOS/Blueprint",
  "attributes": [
    {
      "name": "name",
      "type": "system/SIMOS/BlueprintAttribute",
      "description": "sima application service configuration name.",
      "default": "simulationServer",
      "contained": true,
      "optional": false,
      "attributeType": "string"
    },
    {
      "name": "description",
      "type": "system/SIMOS/BlueprintAttribute",
      "description": "The description of the simulation server",
      "default": "This is abel.",
      "contained": true,
      "optional": false,
      "attributeType": "string"
    },
    {
      "name": "type",
      "type": "system/SIMOS/BlueprintAttribute",
      "description": "simulation config type",
      "attributeType": "string"
    },
    {
      "name": "platform",
      "type": "system/SIMOS/BlueprintAttribute",
      "description": "platform name",
      "default": "Asgard B",
      "attributeType": "string"
    },
    {
      "name": "application",
      "type": "system/SIMOS/BlueprintAttribute",
      "description": "application name",
      "default": "Mooring SRS",
      "attributeType": "string"
    },
    {
      "name": "task",
      "type": "system/SIMOS/BlueprintAttribute",
      "description": "task name",
      "default": "Task",
      "attributeType": "string"
    },
    {
      "name": "workflow",
      "type": "system/SIMOS/BlueprintAttribute",
      "description": "workflow name",
      "default": "ULS",
      "attributeType": "string"
    },
    {
      "name": "address",
      "type": "system/SIMOS/BlueprintAttribute",
      "description": "server address.",
      "default": "http://abel.sintef.no",
      "attributeType": "string"
    },
    {
      "name": "port",
      "type": "system/SIMOS/BlueprintAttribute",
      "description": "port number",
      "default": "8085",
      "attributeType": "integer"
    }
  ],
  "storageRecipes": [],
  "uiRecipes": [
    {
      "type": "system/SIMOS/UiRecipe",
      "name": "SIMA Application Service Config",
      "description": "SIMA App",
      "plugin": "simpos-single-object-form",
      "attributes": []
    },

    {
      "name": "Raw",
      "type": "system/SIMOS/UiRecipe",
      "description": "",
      "plugin": "PREVIEW",
      "field": "",
      "hideTab": true,
      "options": [],
      "attributes": []
    }
  ]
}
