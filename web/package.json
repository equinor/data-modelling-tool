    "@types/victory": "^33.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "jest-puppeteer": "^4.4.0",
    "pptr-testing-library": "^0.4.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "puppeteer": "^2.0.0",
    "react-test-renderer": "^16.8.6",
    "require-context.macro": "^1.0.4",
    "ts-jest": "^24.2.0"
