  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "tsc": "tsc",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prettier": "prettier --write './src/**/*.{ts,tsx,js,css,html,json}'",
    "prettier:home": "prettier --write '../api/home/**/*.json'",
    "test:integration": "jest -c integration/jest.config.js",
    "test:integrationWithWatch": "jest -c integration/jest.config.js --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run coverage"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addon-storyshots": "^5.3.13",
